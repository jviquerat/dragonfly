{
    "env": {
        "name": "Pendulum-v1",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "n_avg": 3,
    "n_cpu": 1,
    "n_ep_max": 300,
    "trainer": {
        "style": "td",
        "mem_size": 10000,
        "btc_size": 128,
        "n_stp_unroll": 1,
        "render_every": 500
    },
    "agent": {
        "type": "ddpg",
        "gamma": 0.99,
        "rho": 0.995,
        "n_warmup": 2000,
        "n_filling": 1000,
        "sigma": 0.1,
        "policy": {
            "type": "deterministic",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "selu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["selu"],
                    "final": ["tanh"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 5.0e-4,
                "grd_clip": null
            },
            "loss": {
                "type": "q_pol"
            }
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "selu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["selu"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 5.0e-4,
                "grd_clip": null
            },
            "loss": {
                "type": "mse_ddpg"
            }
        },
        "termination": {
            "type": "bootstrap",
            "ep_end": 200
        }
    }
}
