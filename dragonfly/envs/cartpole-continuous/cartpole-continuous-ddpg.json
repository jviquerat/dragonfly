{
    "env": {
        "name": "envs.cartpole:cartpole-continuous-v0",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "name_env": true,
    "name_agent": true,
    "name_tag": "grd_1",
    "name_time": false,
    "n_avg": 3,
    "n_cpu": 1,
    "n_ep_max": 500,
    "trainer": {
        "style": "td",
        "mem_size": 20000,
        "btc_size": 64,
        "n_stp_unroll": 1,
        "render_every": 500
    },
    "agent": {
        "type": "ddpg",
        "gamma": 0.99,
        "rho": 0.995,
        "n_warmup": 2500,
        "policy": {
            "type": "deterministic",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "tanh"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["tanh"],
                    "final": ["tanh"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.0e-4,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "q_pol"
            }
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.0e-3,
                "grd_clip": 10000.0
            },
            "loss": {
                "type": "mse_ddpg"
            }
        },
        "termination": {
            "type": "bootstrap",
            "ep_end": 200
        }
    }
}
