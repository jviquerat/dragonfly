{
    "env": {
        "name": "CartPole-v0",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "name_env": true,
    "name_agent": true,
    "name_tag": "",
    "name_time": false,
    "n_avg": 3,
    "n_cpu": 1,
    "n_ep_max": 300,
    "trainer": {
        "style": "td",
        "mem_size": 10000,
        "btc_size": 64,
        "n_stp_unroll": 1,
        "render_every": 500
    },
    "agent": {
        "type": "dqn",
        "gamma": 0.99,
        "tgt_update": 200,
        "exploration": {
            "type": "linear",
            "start": 0.9,
            "end": 0.05,
            "n_decay": 20000
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [32,32],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 5.0e-4,
                "grd_clip": 100.0
            },
            "loss": {
                "type": "mse_dqn"
            }
        },
        "termination": {
            "type": "bootstrap",
            "ep_end": 200
        }
    }
}
