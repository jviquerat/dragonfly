{
    "env": {
        "name": "cartpole:cartpole-continuous-v0",
        "obs_clip": true,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "n_avg": 3,
    "n_cpu": 1,
    "n_ep_max": 300,
    "trainer": {
        "style": "buffer",
        "buff_size": 512,
        "n_buff": 16,
        "batch_frac": 0.5,
        "n_epochs": 16,
        "render_every": 500
    },
    "agent": {
        "type": "ppo",
        "policy": {
            "type": "beta",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [32,32],
                    "actv": "tanh"
                },
                "heads": {
                    "nb": 2,
                    "arch": [[16],[16]],
                    "actv": ["tanh","tanh"],
                    "final": ["sigmoid","sigmoid"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.0e-3,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "surrogate",
                "pol_clip": 0.2,
                "ent_coef": 0.01
            }
        },
        "value": {
            "type": "v_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [32,32],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[16]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 5.0e-3,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "mse_pg"
            }
        },
        "retrn": {
            "type": "gae",
            "gamma": 0.99,
            "gae_lambda": 0.99,
            "ret_norm": true
        },
        "terminator": {
            "type": "bootstrap",
            "ep_end": 200
        }
    }
}
