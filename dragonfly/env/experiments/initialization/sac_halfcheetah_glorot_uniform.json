{
    "env": {
        "name": "HalfCheetah-v4",
        "obs_clip": false,
        "obs_norm": false,
        "obs_noise": 0.0
    },
    "naming": {
        "env": true,
        "agent": true,
        "tag": "glorot_uniform",
        "time": false
    },
    "n_avg": 3,
    "n_stp_max": 1000000,
    "trainer": {
        "style": "td",
        "mem_size": 1000000,
        "btc_size": 100,
        "n_stp_unroll": 50,
        "render_every": 500000
    },
    "agent": {
        "type": "sac_auto",
        "gamma": 0.99,
        "rho": 0.995,
        "n_warmup": 10000,
        "n_filling": 1000,
        "alpha": {
            "alpha_0": 0.01,
            "monitor": true,
            "optimizer": {
                "type": "adam",
                "lr": 3.0e-4,
                "grd_clip": null
            }
        },
        "policy": {
	          "type": "tanh_normal",
            "min_log_std": -20.0,
            "max_log_std": 2.0,
            "network": {
                "type": "fc",
                "k_init": "glorot_uniform",
                "k_final": "glorot_uniform",
                "trunk": {
                    "arch": [256],
                    "actv": "relu"
                },
                "heads": {
                    "nb": 2,
                    "arch": [[256],[256]],
                    "actv": ["relu","relu"],
                    "final": ["linear","linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 3.0e-4,
                "grd_clip": null
            },
            "loss": {
                "type": "q_pol_sac"
            }
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "k_init": "glorot_uniform",
                "k_final": "glorot_uniform",
                "trunk": {
                    "arch": [256,256],
                    "actv": "relu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["relu"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 3.0e-4,
                "grd_clip": null
            },
            "loss": {
                "type": "mse_sac"
            }
        },
        "termination": {
            "type": "bootstrap"
        }
    }
}
