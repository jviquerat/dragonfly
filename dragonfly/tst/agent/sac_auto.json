{
    "env": {
        "name": "Pendulum-v1",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "n_avg": 3,
    "n_stp_max": 500,
    "trainer": {
        "style": "td",
        "mem_size": 20000,
        "btc_size": 50,
        "n_stp_unroll": 25,
        "render_every": 500
    },
    "agent": {
        "type": "sac_auto",
        "gamma": 0.99,
        "rho": 0.995,
        "n_warmup": 200,
        "n_filling": 200,
        "alpha": {
            "alpha_0": 0.1,
            "optimizer": {
                "type": "adam",
                "lr": 1.0e-3,
                "grd_clip": null
            }
        },
        "policy": {
            "type": "tanh_normal",
            "min_log_std": -20.0,
            "max_log_std": 2.0,
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64],
                    "actv": "relu"
                },
                "heads": {
                    "nb": 2,
                    "arch": [[64],[64]],
                    "actv": ["relu","relu"],
                    "final": ["linear","linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 1.0e-3,
                "grd_clip": null
            },
            "loss": {
                "type": "q_pol_sac"
            }
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "relu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["relu"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 1.0e-3,
                "grd_clip": null
            },
            "loss": {
                "type": "mse_sac"
            }
        },
        "termination": {
            "type": "regular"
        }
    }
}
