{
    "env": {
        "name": "Pendulum-v1",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "n_avg": 3,
    "n_stp_max": 500,
    "trainer": {
        "style": "td",
        "mem_size": 20000,
        "btc_size": 64,
        "n_stp_unroll": 1,
        "render_every": 500
    },
    "agent": {
        "type": "td3",
        "gamma": 0.99,
        "rho": 0.995,
        "n_warmup": 200,
        "n_filling": 100,
        "p_update": 2,
        "sigma_act": 0.1,
        "sigma_tgt": 0.1,
        "noise_clip": 0.2,
        "policy": {
            "type": "deterministic",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "tanh"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["tanh"],
                    "final": ["tanh"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 5.0e-4,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "q_pol"
            }
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 5.0e-4,
                "grd_clip": 100.0
            },
            "loss": {
                "type": "mse_td3"
            }
        },
        "termination": {
            "type": "regular"
        }
    }
}
