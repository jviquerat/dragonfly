{
    "env_name": "CartPole-v0",
    "n_avg": 3,
    "n_cpu": 1,
    "n_ep_max": 20,
    "trainer": {
        "style": "td",
        "mem_size": 20000,
        "btc_size": 64,
        "render_every": 500,
        "terminator": {
            "type": "regular"
        }
    },
    "agent": {
        "type": "dqn",
        "target_freq": 10,
        "gamma": 0.99,
        "exploration": {
            "type": "linear",
            "start": 0.1,
            "end": 0.01,
            "n_steps": 50000
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [32,32],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.0e-3,
                "grd_clip": 0.5
            },
            "loss": {
                "type": "mse_q"
            }
        }
    }
}
