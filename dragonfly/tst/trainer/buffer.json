{
    "env": {
        "name": "CartPole-v0",
        "obs_clip": true,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "n_avg": 2,
    "n_stp_max": 500,
    "trainer": {
        "style": "buffer",
        "buff_size": 128,
        "n_buff": 8,
        "batch_frac": 0.5,
        "n_epochs": 8,
        "render_every": 500
    },
    "agent": {
        "type": "ppo",
        "policy": {
            "type": "categorical",
            "network": {
                "type": "fc",
                "k_init": "orthogonal1",
                "k_final": "orthogonal0",
                "trunk": {
                    "arch": [16,16],
                    "actv": "tanh"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["tanh"],
                    "final": ["softmax"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 2.0e-3,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "surrogate",
                "pol_clip": 0.1,
                "ent_coef": 0.01
            }
        },
        "value": {
            "type": "v_value",
            "network": {
                "type": "fc",
                "k_init": "orthogonal1",
                "k_final": "orthogonal0",
                "trunk": {
                    "arch": [16,16],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.0e-2,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "mse_pg"
            }
        },
        "retrn": {
            "type": "gae",
            "gamma": 0.99,
            "gae_lambda": 0.99,
            "ret_norm": true
        },
        "termination": {
            "type": "bootstrap",
            "ep_end": 200
        }
    }
}
