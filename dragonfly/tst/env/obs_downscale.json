{
    "env": {
        "name": "CarRacing-v3",
        "obs_clip": false,
        "obs_norm": false,
        "obs_noise": 0.0,
        "obs_downscale": 4
    },
    "naming": {
        "env": true,
        "agent": true,
        "tag": "cnn",
        "time": false
    },
    "n_avg": 2,
    "n_stp_max": 20,
    "trainer": {
        "style": "buffer",
        "buff_size": 10,
        "n_buff": 1,
        "batch_frac": 0.5,
        "n_epochs": 4,
        "render_every": 50000
    },
    "agent": {
        "type": "ppo",
        "policy": {
            "type": "normal",
            "network": {
                "type": "conv2d",
                "trunk": {
                    "filters": [8,8],
		                "kernels": [7,3],
		                "stride": 1,
                    "pooling": false,
                    "actv": "relu"
                },
                "heads": {
                    "nb": 2,
                    "arch": [[64],[64]],
                    "actv": ["tanh","tanh"],
                    "final": ["tanh","sigmoid"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 2.0e-4,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "surrogate",
                "pol_clip": 0.2,
                "ent_coef": 0.01
            }
        },
        "value": {
            "type": "v_value",
            "network": {
                "type": "conv2d",
                "trunk": {
                    "filters": [8,8],
		                "kernels": [7,3],
		                "stride": 1,
                    "pooling": false,
                    "actv": "relu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[64]],
                    "actv": ["relu"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 2.0e-4,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "mse_pg"
            }
        },
        "retrn": {
            "type": "gae",
            "gamma": 0.99,
            "gae_lambda": 0.95,
            "ret_norm": true
        },
        "termination": {
            "type": "bootstrap"
        }
    }
}
