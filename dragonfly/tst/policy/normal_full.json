{
    "policy": {
        "type": "normal_full",
        "network": {
            "type": "fc",
            "trunk": {
                "arch": [32,32],
                "actv": "swish"
            },
            "heads": {
                "nb": 3,
                "arch": [[16],[16],[16]],
                "actv": ["swish","swish","swish"],
                "final": ["tanh","sigmoid","sigmoid"]
            }
        },
        "optimizer": {
            "type": "nadam",
            "lr": 1.0e-3,
            "grd_clip": 0.1
        },
        "loss": {
            "type": "surrogate",
            "pol_clip": 0.1,
            "ent_coef": 0.01
        }
    }
}
