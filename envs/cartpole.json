{
    "env_name": "CartPole-v0",
    "agent": "ppo",
    "ep_end": 200,
    "n_ep": 300,
    "n_avg": 5,
    "n_cpu": 1,
    "render_every": 500,
    "policy": {
        "type": "multinomial",
        "network": {
            "type": "fc",
            "arch": [16,16]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 1.0e-2,
            "grd_clip": 0.5
        }
    },
    "value": {
        "type": "v_value",
        "network": {
            "type": "fc",
            "arch": [16,16]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 1.0e-2,
            "grd_clip": 0.5
        }
    },
    "retrn": {
        "type": "gae",
        "gamma": 0.99,
        "gae_lambda": 0.99,
        "ret_norm": true,
        "ret_clip": true
    },
    "batch_frac": 0.5,
    "n_buff": 8,
    "n_epochs": 8,
    "buff_size": 128,
    "pol_clip": 0.1,
    "entropy": 0.01,
    "bootstrap": true
}
