{
    "env_name": "CartPole-v0",
    "agent": "ppo",
    "ep_end": 200,
    "n_ep": 300,
    "n_avg": 2,
    "n_cpu": 1,
    "render_every": 500,
    "policy": {
        "type": "multinomial",
        "network": {
            "arch": [16,16],
            "fnl_actv": "softmax"
        },
        "optimizer": {
            "lr": 1.0e-2,
            "grd_clip": 0.5
        }
    },
    "value": {
        "type": "v_value",
        "network": {
            "arch": [16,16],
            "fnl_actv": "linear"
        },
        "optimizer": {
            "lr": 1.0e-2,
            "grd_clip": 0.5
        }
    },
    "batch_frac": 0.5,
    "n_buff": 8,
    "n_epochs": 8,
    "buff_size": 128,
    "pol_clip": 0.1,
    "entropy": 0.01,
    "gamma": 0.99,
    "gae_lambda": 0.99,
    "adv_norm": true,
    "adv_clip": true,
    "bootstrap": true
}
