{
    "env_name": "LunarLander-v2",
    "agent": "ppo",
    "ep_end": 400,
    "n_ep": 1500,
    "n_avg": 2,
    "n_cpu": 1,
    "render_every": 500,
    "policy": {
        "type": "categorical",
        "network": {
            "type": "fc",
            "arch": [32,16]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "ppo",
            "pol_clip": 0.1,
            "ent_coef": 0.01
        }
    },
    "value": {
        "type": "v_value",
        "network": {
            "type": "fc",
            "arch": [16,16]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "mse"
        }
    },
    "retrn": {
        "type": "gae",
        "gamma": 0.99,
        "gae_lambda": 0.99,
        "ret_norm": true,
        "ret_clip": true
    },
    "batch_frac": 0.5,
    "n_buff": 8,
    "n_epochs": 8,
    "buff_size": 1024,
    "bootstrap": true
}
