{
    "env_name": "CartPole-v0",
    "n_avg": 5,
    "n_cpu": 1,
    "n_ep_max": 1000,
    "trainer": {
        "style": "td",
        "mem_size": 10000,
        "btc_size": 64,
        "render_every": 500,
        "terminator": {
            "type": "bootstrap",
            "ep_end": 200
        }
    },
    "agent": {
        "type": "dqn",
        "target_freq": 100,
        "gamma": 0.99,
        "exploration": {
            "type": "linear",
            "start": 0.1,
            "end": 0.05,
            "n_steps": 500
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [32,32],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 5.0e-4,
                "grd_clip": 0.5
            },
            "loss": {
                "type": "mse"
            }
        }
    }
}
