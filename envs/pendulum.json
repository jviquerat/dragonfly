{
    "env_name": "Pendulum-v0",
    "agent": "ppo",
    "ep_end": 200,
    "n_ep": 300,
    "n_avg": 2,
    "n_cpu": 1,
    "render_every": 500,
    "policy": {
        "type": "normal",
        "network": {
            "type": "fc",
            "trunk": {
                "arch": [32],
                "actv": "tanh"
            },
            "heads": {
                "nb": 2,
                "arch": [[32],[32]],
                "actv": ["tanh","tanh"],
                "final": ["tanh","sigmoid"]
            }
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "ppo",
            "pol_clip": 0.1,
            "ent_coef": 0.1
        }
    },
    "value": {
        "type": "v_value",
        "network": {
            "type": "fc",
            "trunk": {
                "arch": [32,32],
                "actv": "tanh"
            },
            "heads": {
                "nb": 1,
                "arch": [[]],
                "actv": ["tanh"],
                "final": ["linear"]
            }
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "mse"
        }
    },
    "retrn": {
        "type": "gae",
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "ret_norm": true,
        "ret_clip": true
    },
    "batch_frac": 0.5,
    "n_buff": 8,
    "n_epochs": 8,
    "buff_size": 1024,
    "bootstrap": true
}
