{
    "env_name": "Pendulum-v0",
    "agent": "ppo",
    "ep_end": 200,
    "n_ep": 300,
    "n_avg": 4,
    "n_cpu": 1,
    "render_every": 50,
    "policy": {
        "type": "normal",
        "mu_network": {
            "type": "fc",
            "arch": [32,32]
        },
        "sg_network": {
            "type": "fc",
            "arch": [32,32]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "ppo",
            "pol_clip": 0.2,
            "ent_coef": 0.05
        }
    },
    "value": {
        "type": "v_value",
        "network": {
            "type": "fc",
            "arch": [32,32]
        },
        "optimizer": {
            "type": "nadam",
            "lr": 5.0e-3,
            "grd_clip": 0.5
        },
        "loss": {
            "type": "mse"
        }
    },
    "retrn": {
        "type": "gae",
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "ret_norm": true,
        "ret_clip": true
    },
    "batch_frac": 0.5,
    "n_buff": 8,
    "n_epochs": 8,
    "buff_size": 128,
    "bootstrap": true
}
