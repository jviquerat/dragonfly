{
    "env_name": "BipedalWalker-v3",
    "n_avg": 2,
    "n_cpu": 1,
    "n_ep_max": 3000,
    "trainer": {
        "style": "buffer",
        "buff_size": 2048,
        "n_buff": 4,
        "batch_frac": 0.5,
        "n_epochs": 8,
        "render_every": 500,
        "terminator": {
            "type": "bootstrap",
            "ep_end": 1600
        }
    },
    "agent": {
        "type": "ppo",
        "policy": {
            "type": "normal",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "tanh"
                },
                "heads": {
                    "nb": 2,
                    "arch": [[32],[32]],
                    "actv": ["tanh","tanh"],
                    "final": ["tanh","sigmoid"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 2.5e-4,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "surrogate",
                "pol_clip": 0.1,
                "ent_coef": 0.005
            }
        },
        "value": {
            "type": "v_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "swish"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["swish"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "nadam",
                "lr": 1.25e-3,
                "grd_clip": 0.1
            },
            "loss": {
                "type": "mse"
            }
        },
        "retrn": {
            "type": "gae",
            "gamma": 0.99,
            "gae_lambda": 0.99,
            "ret_norm": true,
            "ret_clip": false
        }
    }
}
